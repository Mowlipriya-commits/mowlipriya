def checkUgly(n):
    list1=[]
    for i in range(2,n+1):
        c=0
        if n%i==0:
            for j in range(1,i+1):
                if i%j==0:
                    c+=1
            if c==2:
                list1.append(i)
    list2=[2,3,5]       
    count=0
    for i in list1:
        if i not in list2:
            count+=1
    if count==0:
        return "ugly"
    else:
        return "not ugly"
